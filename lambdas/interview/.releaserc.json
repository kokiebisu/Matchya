{
    "extends": "semantic-release-monorepo",
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "releaseRules": [
            { "type": "feat", "scope": "interview", "release": "minor" },
            { "type": "fix", "scope": "interview", "release": "patch" },
            { "type": "chore", "scope": "interview", "release": "patch" },
            { "type": "feat", "scope": "!interview", "release": false },
            { "type": "fix", "scope": "!interview", "release": false },
            { "type": "chore", "scope": "!interview", "release": false }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
          "@semantic-release/npm",
          {
            "npmPublish": false
          }
      ],
      "@semantic-release/git",
      "@semantic-release/github"
    ],
    "branches": [
      "staging"
    ],
    "tagFormat": "interview-v${version}"
  }
  